<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
			 xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
			 xmlns:vm="clr-namespace:MVVM_Login.ViewModels"
			 x:Class="MVVM_Login.Views.MorePage">
	<ContentPage.BindingContext>
		<vm:MoreViewModel />
	</ContentPage.BindingContext>
	<TableView Intent="Settings">
        <TableRoot>
            <TableSection Title="More :">
                <TextCell Text="1) License" Tapped="OnLicenseTapped" />
                <TextCell Text="2) Data Protection" Tapped="OnDataProtectionTapped" />
                <TextCell Text="3) Publisher" Tapped="OnPublisherTapped" />
                <TextCell Text="4) Legal Notices" Tapped="OnLegalNoticesTapped" />
            </TableSection>
        </TableRoot>
    </TableView>
</ContentPage>